      "minecraft:look_to_start_drowned_transformation": {
        "minecraft:environment_sensor": {
          "triggers": {
            "filters": {
              "test": "is_underwater", "subject": "self", "operator": "==", "value": true
            },
            "event": "minecraft:start_transforming"
          }
        }
      },
